    <xs:import namespace="urn:pl:oire:businessDataTypes:v1" schemaLocation="../businessDataTypes.xsd"/>
    <xs:import namespace="urn:pl:oire:technical:v1" schemaLocation="../technical.xsd"/>
    
    <!-- ====================================================================================================================================================== -->
    <!-- ==== 3.1.1.1. Powiadomienie o zmianie charakterystyki PP                                                                                          ==== -->
    <!-- ====================================================================================================================================================== -->
    <xs:element name="MeteringPointCharacteristicModificationNotification" type="unk_3_1_1_1:MeteringPointCharacteristicModificationNotification"/>
    <xs:complexType name="MeteringPointCharacteristicModificationNotification">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>3.1.1.1. Powiadomienie o zmianie charakterystyki PP</unk_3_1_1_1:plName>
            </xs:appinfo>
        </xs:annotation>
        <xs:sequence>
            <!-- Nagłówek -->
            <xs:element name="Header" type="tech:Header" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Nagłówek</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-700</unk_3_1_1_1:code>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            
            <!-- Kontekst -->
            <xs:element name="ProcessEnergyContext" type="tech:ProcessEnergyContext" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Kontekst</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-715</unk_3_1_1_1:code>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            
            <!-- 3.1.1.1. Payload Powiadomienie o zmianie charakterystyki PP -->
            <xs:element name="Payload" type="unk_3_1_1_1:MeteringPointCharacteristicModificationNotificationPayload" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>3.1.1.1. Payload Powiadomienie o zmianie charakterystyki PP</unk_3_1_1_1:plName>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <!-- ====================================================================================================================================================== -->
    <!-- ==== 3.1.1.1. Payload Powiadomienie o zmianie charakterystyki PP                                                                                  ==== -->
    <!-- ====================================================================================================================================================== -->
    <xs:element name="MeteringPointCharacteristicModificationNotificationPayload" type="unk_3_1_1_1:MeteringPointCharacteristicModificationNotificationPayload"/>
    <xs:complexType name="MeteringPointCharacteristicModificationNotificationPayload">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>3.1.1.1. Payload Powiadomienie o zmianie charakterystyki PP</unk_3_1_1_1:plName>
            </xs:appinfo>
        </xs:annotation>
        <xs:sequence>
            <!-- Pozostałe elementy komunikatu -->
            <xs:element name="Miscellaneous" type="unk_3_1_1_1:Miscellaneous" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Pozostałe elementy komunikatu</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-320</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 1.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Dane PP - ogólne -->
            <xs:element name="MeteringPointData_Basic" type="unk_3_1_1_1:MeteringPointData_Basic" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Dane PP - ogólne</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-300</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 2.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Dane PP - podmioty obsługujące PP -->
            <xs:element name="MeteringPointData_Operators" type="unk_3_1_1_1:MeteringPointData_Operators" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Dane PP - podmioty obsługujące PP</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-303</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 3.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Parametry techniczne - ogólne -->
            <xs:element name="TechnicalData_Basic" type="unk_3_1_1_1:TechnicalData_Basic" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Parametry techniczne - ogólne</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-304</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 4.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Własność elementów układu pomiarowo-rozliczeniowego -->
            <xs:element name="MeasurementAndBillingCircuitOwnership" type="unk_3_1_1_1:MeasurementAndBillingCircuitOwnership" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Własność elementów układu pomiarowo-rozliczeniowego</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-310</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 5.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Dane Użytkownika KSE - ogólne -->
            <xs:element name="KseUserData_Basic" type="unk_3_1_1_1:KseUserData_Basic" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Dane Użytkownika KSE - ogólne</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-395</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 6.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Dane Użytkownika KSE - podstawowe -->
            <xs:element name="KseUserData_Primary" type="unk_3_1_1_1:KseUserData_Primary" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Dane Użytkownika KSE - podstawowe</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-311</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 7.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Parametry handlowe - ogólne -->
            <xs:element name="BusinessData_Basic" type="unk_3_1_1_1:BusinessData_Basic" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Parametry handlowe - ogólne</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-316</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 8.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <!-- ====================================================================================================================================================== -->
    <!-- ==== Pozostałe elementy komunikatu                                                                                                                ==== -->
    <!-- ====================================================================================================================================================== -->
    <xs:element name="Miscellaneous" type="unk_3_1_1_1:Miscellaneous"/>
    <xs:complexType name="Miscellaneous">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>Pozostałe elementy komunikatu</unk_3_1_1_1:plName>
                <unk_3_1_1_1:code>PL-320</unk_3_1_1_1:code>
            </xs:appinfo>
            <xs:documentation xml:lang="pl">Sekcja 1.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- ID_instancji_procesu_referencyjnego -->
            <xs:element name="ProcessInstanceId" type="bdt:UuidType" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>ID_instancji_procesu_referencyjnego</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-002</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Unikatowy kod instancji procesu referencyjnego komunikatu, nadawany przez CSIRE.</xs:documentation>
                    <xs:documentation xml:lang="en">The unique process instance code of the message reference process, assigned by CSIRE.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Data aktualizacji atrybutów -->
            <xs:element name="EffectiveDate" type="xs:date" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Data aktualizacji atrybutów</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-142</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Data ostatniej zmiany charakterystyki rozumiana jako dzień, od którego obowiązują zmienione atrybuty charakterystyki PP.</xs:documentation>
                    <xs:documentation xml:lang="en">Date of the last change of characteristics understood as the day from which the changed attributes of the MP characteristics are effective.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <!-- ====================================================================================================================================================== -->
    <!-- ==== Dane PP - ogólne                                                                                                                             ==== -->
    <!-- ====================================================================================================================================================== -->
    <xs:element name="MeteringPointData_Basic" type="unk_3_1_1_1:MeteringPointData_Basic"/>
    <xs:complexType name="MeteringPointData_Basic">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>Dane PP - ogólne</unk_3_1_1_1:plName>
                <unk_3_1_1_1:code>PL-300</unk_3_1_1_1:code>
            </xs:appinfo>
            <xs:documentation xml:lang="pl">Sekcja 2.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- Kod PP -->
            <xs:element name="MeteringPointCode" type="bdt:MeteringPointCodeType" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Kod PP</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-001</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Kod identyfikujący Punkt pomiarowy - zgodny ze standardem GS1.</xs:documentation>
                    <xs:documentation xml:lang="en">Metering Point Identification Code - compliant with the GS1 standard.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Typ Punktu pomiarowego -->
            <xs:element name="MeteringPointType" type="bdt:MeteringPointType_Type" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Typ Punktu pomiarowego</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-053</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie typu Punktu pomiarowego, zgodnie ze słownikiem.</xs:documentation>
                    <xs:documentation xml:lang="en">Determination of the Metering Point type, according to the dictionary.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Charakter PPE -->
            <xs:element name="MpApType" type="bdt:MpApType_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Charakter PPE</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-056</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie charakteru Punktu poboru energii, zgodnie ze słownikiem.</xs:documentation>
                    <xs:documentation xml:lang="en">Determination of the nature of the Energy Consumption Point, according to the dictionary.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Charakter PPI -->
            <xs:element name="MpOtherType" type="bdt:MpOtherType_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Charakter PPI</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-419</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie charakteru Punktu pomiarowego inny, zgodnie ze słownikiem.</xs:documentation>
                    <xs:documentation xml:lang="en">Determination of the nature of the Metering Point other, according to the dictionary.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Czy PP podrzędny względem innego PP? -->
            <xs:element name="IsChildMp" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Czy PP podrzędny względem innego PP?</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-418</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie, czy wskazany Punkt pomiarowy jest autonomiczny, czy jest podrzędny względem innego Punktu pomiarowego.</xs:documentation>
                    <xs:documentation xml:lang="en">Determination whether the indicated Metering Point is autonomous or is subordinate to another Metering Point.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Kod nadrzędnego PP -->
            <xs:element name="ParentMeteringPointCode" type="bdt:MeteringPointCodeType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Kod nadrzędnego PP</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-420</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Kod identyfikujący Punkt pomiarowy nadrzędny wobec zgłaszanego PP - zgodny ze standardem GS1.</xs:documentation>
                    <xs:documentation xml:lang="en">Code identifying the Metering Point superior to the reported MP - compliant with the GS1 standard.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Czy PP wchodzi w skład obiektu? -->
            <xs:element name="IsMpPartOfFacility" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Czy PP wchodzi w skład obiektu?</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-421</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie, czy dany Punkt pomiarowy jest elementem obiektu pomiarowego.</xs:documentation>
                    <xs:documentation xml:lang="en">Determination of whether a given Metering Point is part of a metering facility.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Dane PP - obiekt -->
            <xs:element name="MeteringPointData_Facility" type="unk_3_1_1_1:MeteringPointData_Facility" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Dane PP - obiekt</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-422</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 2.1.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Dane PP - adresowe -->
            <xs:element name="MeteringPointData_Address" type="unk_3_1_1_1:MeteringPointData_Address" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Dane PP - adresowe</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-301</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 2.2.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Dane PP - obszar sieci pomiarowej -->
            <xs:element name="MeteringPointData_Area" type="unk_3_1_1_1:MeteringPointData_Area" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Dane PP - obszar sieci pomiarowej</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-302</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 2.3.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Dane PP - PPW -->
            <xs:element name="MeteringPointData_ExchangePoint" type="unk_3_1_1_1:MeteringPointData_ExchangePoint" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Dane PP - PPW</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-539</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 2.4.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <!-- ====================================================================================================================================================== -->
    <!-- ==== Dane PP - obiekt                                                                                                                             ==== -->
    <!-- ====================================================================================================================================================== -->
    <xs:element name="MeteringPointData_Facility" type="unk_3_1_1_1:MeteringPointData_Facility"/>
    <xs:complexType name="MeteringPointData_Facility">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>Dane PP - obiekt</unk_3_1_1_1:plName>
                <unk_3_1_1_1:code>PL-422</unk_3_1_1_1:code>
            </xs:appinfo>
            <xs:documentation xml:lang="pl">Sekcja 2.1.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- Typ obiektu -->
            <xs:element name="FacilityType" type="bdt:FacilityType_Type" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Typ obiektu</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-423</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie typu obiektu, zgodnie ze słownikiem.</xs:documentation>
                    <xs:documentation xml:lang="en">Determination of the facility type, according to the dictionary.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Identyfikator obiektu -->
            <xs:element name="FacilityIdentifier" type="bdt:FacilityIdentifierType" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Identyfikator obiektu</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-424</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Unikalny kod identyfikujący obiekt pomiarowy. Struktura Identyfikatora obiektu składa się z:
1) kodu EIC (np. 19X000000000001C),
2) oznaczenia przedmiotu identyfikacji (OP - obiektu pomiarowego),
3) unikalnego dopełnienia nadawanego przez Operatora (np. 11111111).</xs:documentation>
                    <xs:documentation xml:lang="en">Unique code identifying the metering facility. The structure of the facility Identifier consists of:
1) EIC code (e.g. 19X000000000001C),
2) designation of the facility of identification (OP - measurement facility),
3) a unique complement assigned by the Operator (e.g. 11111111).</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <!-- ====================================================================================================================================================== -->
    <!-- ==== Dane PP - adresowe                                                                                                                           ==== -->
    <!-- ====================================================================================================================================================== -->
    <xs:element name="MeteringPointData_Address" type="unk_3_1_1_1:MeteringPointData_Address"/>
    <xs:complexType name="MeteringPointData_Address">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>Dane PP - adresowe</unk_3_1_1_1:plName>
                <unk_3_1_1_1:code>PL-301</unk_3_1_1_1:code>
            </xs:appinfo>
            <xs:documentation xml:lang="pl">Sekcja 2.2.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- Kraj -->
            <xs:element name="Country" type="bdt:CountryIsoCodeType" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Kraj</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-003</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Kod kraju zgodny z ISO 3166-1 alpha-2</xs:documentation>
                    <xs:documentation xml:lang="en">Country code in accordance with ISO 3166-1 alpha-2</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Miejscowość -->
            <xs:element name="CityName" type="bdt:String50_Type" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Miejscowość</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-004</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Nazwa miejscowości (właściwa dla Punktu pomiarowego).</xs:documentation>
                    <xs:documentation xml:lang="en">Name of the city (specific to the Metering Point).</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Czy zastosowano podział na ulice? -->
            <xs:element name="IsStreetSeparationPresent" type="xs:boolean" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Czy zastosowano podział na ulice?</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-558</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie, czy dla danego adresu PP w miejscowości zastosowano podział na ulice.</xs:documentation>
                    <xs:documentation xml:lang="en">Determination whether street division is applicable for this MP address</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Czy ulica zgodna z TERYT? -->
            <xs:element name="IsStreetTerytCodeAvailable" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Czy ulica zgodna z TERYT?</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-584</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie, czy dla danej ulicy istnieje kod TERYT</xs:documentation>
                    <xs:documentation xml:lang="en">Determines whether a TERYT code exists for a given street</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Kod pocztowy -->
            <xs:element name="PostalCode" type="bdt:PostalCodeType" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Kod pocztowy</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-005</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Kod pocztowy</xs:documentation>
                    <xs:documentation xml:lang="en">Postal code</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Ulica -->
            <xs:element name="StreetName" type="bdt:String100_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Ulica</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-006</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Nazwa ulicy albo nazwa miejscowości, w przypadku miejscowości bez zastosowanego podziału na ulice.</xs:documentation>
                    <xs:documentation xml:lang="en">The name of the street or the name of the city, in the case of a city without street division.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Nr budynku -->
            <xs:element name="BuildingNumber" type="bdt:String10_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Nr budynku</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-007</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Numer budynku</xs:documentation>
                    <xs:documentation xml:lang="en">Building number</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Nr lokalu / mieszkania -->
            <xs:element name="ApartmentNumber" type="bdt:String10_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Nr lokalu / mieszkania</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-008</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Numer lokalu/mieszkania</xs:documentation>
                    <xs:documentation xml:lang="en">Premises/apartment number</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Nr działki -->
            <xs:element name="PlotNumber" type="bdt:String100_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Nr działki</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-009</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Numer działki (w przypadku braku nazwy ulicy lub numeru budynku).</xs:documentation>
                    <xs:documentation xml:lang="en">Parcel number (in case there is no street name or building number).</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- TERYT -->
            <xs:element name="Teryt" type="bdt:TerytType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>TERYT</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-010</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Zgodny z Krajowym Rejestrem TERYT typy centralny katalog ulic (ULIC).</xs:documentation>
                    <xs:documentation xml:lang="en">Compliant with the National TERYT Register types central street directory (ULIC).</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Szerokość geograficzna -->
            <xs:element name="Latitude" type="bdt:LatitudeType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Szerokość geograficzna</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-012</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Szerokość geograficzna opisująca położenie PP zapisana w standardzie WGS84 w formacie DMF.</xs:documentation>
                    <xs:documentation xml:lang="en">The latitude describing the location of the MP recorded in the WGS84 standard in DMF format.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Długość geograficzna -->
            <xs:element name="Longitude" type="bdt:LongitudeType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Długość geograficzna</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-013</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Długość geograficzna opisująca położenie PP zapisana w standardzie WGS84 w formacie DMF.</xs:documentation>
                    <xs:documentation xml:lang="en">Longitude describing the location of the MP recorded in the WGS84 standard in DMF format.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
