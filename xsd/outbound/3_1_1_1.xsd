    <xs:complexType name="MeteringPointData_Operators">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>Dane PP - podmioty obsługujące PP</unk_3_1_1_1:plName>
                <unk_3_1_1_1:code>PL-303</unk_3_1_1_1:code>
            </xs:appinfo>
