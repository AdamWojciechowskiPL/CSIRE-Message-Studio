                </xs:annotation>
            </xs:element>
            
            <!-- Czy zastosowano podział na ulice? -->
            <xs:element name="IsStreetSeparationPresent" type="xs:boolean" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Czy zastosowano podział na ulice?</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-560</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie, czy dla danego adresu Użytkownika KSE w miejscowości zastosowano podział na ulice.</xs:documentation>
                    <xs:documentation xml:lang="en">Determination whether street division is applicable for this KSE User address.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Czy ulica zgodna z TERYT? -->
            <xs:element name="IsStreetTerytCodeAvailable" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Czy ulica zgodna z TERYT?</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-586</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie, czy dla danej ulicy istnieje kod TERYT</xs:documentation>
                    <xs:documentation xml:lang="en">Determines whether a TERYT code exists for a given street</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Kod pocztowy -->
            <xs:element name="PostalCode" type="bdt:String10_Type" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Kod pocztowy</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-047</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Kod pocztowy</xs:documentation>
                    <xs:documentation xml:lang="en">Postal code</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Ulica -->
            <xs:element name="StreetName" type="bdt:String100_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Ulica</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-048</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Nazwa ulicy albo nazwa miejscowości, w przypadku miejscowości bez zastosowanego podziału na ulice.</xs:documentation>
                    <xs:documentation xml:lang="en">The name of the street or the name of the city, in the case of a city without street division.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Nr budynku -->
            <xs:element name="BuildingNumber" type="bdt:String10_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Nr budynku</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-049</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Numer budynku</xs:documentation>
                    <xs:documentation xml:lang="en">Building number</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Nr lokalu/ mieszkania -->
            <xs:element name="ApartmentNumber" type="bdt:String10_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Nr lokalu/ mieszkania</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-050</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Numer lokalu/mieszkania</xs:documentation>
                    <xs:documentation xml:lang="en">Premises/apartment number</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Nr działki -->
            <xs:element name="PlotNumber" type="bdt:String100_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Nr działki</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-051</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Numer działki (w przypadku braku nazwy ulicy lub numeru budynku).</xs:documentation>
                    <xs:documentation xml:lang="en">Parcel number (in case there is no street name or building number).</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- TERYT -->
            <xs:element name="Teryt" type="bdt:TerytType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>TERYT</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-052</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Zgodny z Krajowym Rejestrem TERYT typy centralny katalog ulic (ULIC).</xs:documentation>
                    <xs:documentation xml:lang="en">Compliant with the National TERYT Register types central street directory (ULIC).</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <!-- ====================================================================================================================================================== -->
    <!-- ==== Dane Użytkownika KSE - dodatkowe                                                                                                             ==== -->
    <!-- ====================================================================================================================================================== -->
    <xs:element name="KseUserData_Additional" type="unk_3_1_1_1:KseUserData_Additional"/>
    <xs:complexType name="KseUserData_Additional">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>Dane Użytkownika KSE - dodatkowe</unk_3_1_1_1:plName>
                <unk_3_1_1_1:code>PL-315</unk_3_1_1_1:code>
            </xs:appinfo>
            <xs:documentation xml:lang="pl">Sekcja 6.3.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- Adres email dla OSD -->
            <xs:element name="DsoEmailAddress" type="bdt:EmailType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Adres email dla OSD</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-035</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Adres poczty elektronicznej, wykorzystywany do realizacji usług dystrybucji na podstawie zawartej UK (między innymi planowane wznowienia dostaw).</xs:documentation>
                    <xs:documentation xml:lang="en">E-mail address, used for the execution of distribution services under the concluded UK (among other things, planned resumption of supply).</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Nr telefonu dla OSD -->
            <xs:element name="DsoPhoneNumber" type="bdt:PhoneNumberType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Nr telefonu dla OSD</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-036</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Numer telefonu kontaktowego, wykorzystywany do realizacji usług dystrybucji na podstawie zawartej UK (między innymi planowane wznowienia dostaw).</xs:documentation>
                    <xs:documentation xml:lang="en">Contact telephone number, used for distribution services under the concluded UK (among other things, planned resumption of supply).</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <!-- ====================================================================================================================================================== -->
    <!-- ==== Dane Użytkownika KSE - podstawowe                                                                                                            ==== -->
    <!-- ====================================================================================================================================================== -->
    <xs:element name="KseUserData_Primary" type="unk_3_1_1_1:KseUserData_Primary"/>
    <xs:complexType name="KseUserData_Primary">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>Dane Użytkownika KSE - podstawowe</unk_3_1_1_1:plName>
                <unk_3_1_1_1:code>PL-311</unk_3_1_1_1:code>
            </xs:appinfo>
            <xs:documentation xml:lang="pl">Sekcja 7.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- Typ Użytkownika KSE -->
            <xs:element name="KseUserType" type="bdt:KseUserType_Type" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Typ Użytkownika KSE</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-024</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie typu Użytkownika KSE wprowadzonego do Punktu pomiarowego.</xs:documentation>
                    <xs:documentation xml:lang="en">Determination of the type of the KSE User entered in the Metering Point.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Imię -->
            <xs:element name="FirstName" type="bdt:String50_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Imię</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-025</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Pierwsze imię użytkownika</xs:documentation>
                    <xs:documentation xml:lang="en">First name of the user</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Nazwisko -->
            <xs:element name="LastName" type="bdt:String100_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Nazwisko</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-026</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Nazwisko użytkownika</xs:documentation>
                    <xs:documentation xml:lang="en">Name of the user.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Nazwa -->
            <xs:element name="CompanyName" type="bdt:String250_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Nazwa</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-027</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Nazwa użytkownika</xs:documentation>
                    <xs:documentation xml:lang="en">User name</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Dane Użytkownika KSE - identyfikacyjne -->
            <xs:element name="KseUserData_Identifiers" type="unk_3_1_1_1:KseUserData_Identifiers" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Dane Użytkownika KSE - identyfikacyjne</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-312</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 7.1.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <!-- ====================================================================================================================================================== -->
    <!-- ==== Dane Użytkownika KSE - identyfikacyjne                                                                                                       ==== -->
    <!-- ====================================================================================================================================================== -->
    <xs:element name="KseUserData_Identifiers" type="unk_3_1_1_1:KseUserData_Identifiers"/>
    <xs:complexType name="KseUserData_Identifiers">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>Dane Użytkownika KSE - identyfikacyjne</unk_3_1_1_1:plName>
                <unk_3_1_1_1:code>PL-312</unk_3_1_1_1:code>
            </xs:appinfo>
            <xs:documentation xml:lang="pl">Sekcja 7.1.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- PESEL -->
            <xs:element name="Pesel" type="bdt:PeselType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>PESEL</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-028</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Numer PESEL identyfikujący użytkownika.</xs:documentation>
                    <xs:documentation xml:lang="en">PESEL number identifying the user.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- NIP -->
            <xs:element name="Nip" type="bdt:NipType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>NIP</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-029</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Numer NIP identyfikujący użytkownika.</xs:documentation>
                    <xs:documentation xml:lang="en">NIP number identifying the user.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- KRS -->
            <xs:element name="Krs" type="bdt:KrsType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>KRS</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-030</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Numer KRS identyfikujący użytkownika.</xs:documentation>
                    <xs:documentation xml:lang="en">KRS number identifying the user.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Typ identyfikatora -->
            <xs:element name="IdentifierType" type="bdt:UserIdentifierType_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Typ identyfikatora</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-032</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sposób nadania identyfikatora osobie fizycznej nierezydent lub rezydent bez PESEL.</xs:documentation>
                    <xs:documentation xml:lang="en">The method of assigning an identifier to an individual non-resident or resident without PESEL.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Inny identyfikator osoby fizycznej -->
            <xs:element name="CustomKseUserIdentifier" type="bdt:IdentityDocumentNumberType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Inny identyfikator osoby fizycznej</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-033</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Identyfikator użytkownika, dla którego nie jest znany PESEL. W przypadku nadania numeru przez Operatora, struktura identyfikatora powinna wyglądać następująco: [EIC] [UKSE] [UNIKATOWE_DOPEŁNIENIE_W_FORMACIE_12345678901]</xs:documentation>
                    <xs:documentation xml:lang="en">Identification for a KSE user without PESEL. The identifier structure should look as follows: [EIC] [UKSE] [UNIQUE_ADDENDUM_IN_FORMAT_12345678901].</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Global NIP -->
            <xs:element name="GlobalTaxIdentification" type="bdt:GlobalTaxIdentificationType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Global NIP</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-034</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Numer identyfikujący użytkownika typu osoba prawna - nierezydent.</xs:documentation>
                    <xs:documentation xml:lang="en">Number identifying the user of the type legal person - non-resident.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <!-- ====================================================================================================================================================== -->
    <!-- ==== Parametry handlowe - ogólne                                                                                                                  ==== -->
    <!-- ====================================================================================================================================================== -->
    <xs:element name="BusinessData_Basic" type="unk_3_1_1_1:BusinessData_Basic"/>
    <xs:complexType name="BusinessData_Basic">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>Parametry handlowe - ogólne</unk_3_1_1_1:plName>
                <unk_3_1_1_1:code>PL-316</unk_3_1_1_1:code>
            </xs:appinfo>
            <xs:documentation xml:lang="pl">Sekcja 8.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- Charakter odbioru -->
            <xs:element name="EnergyConsumptionType" type="bdt:EnergyConsumptionType_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Charakter odbioru</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-057</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie charakteru odbioru, zgodnie ze słownikiem.</xs:documentation>
                    <xs:documentation xml:lang="en">Determination of the nature of the Consumption Point, according to the dictionary.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Szacowany wolumen roczny -->
            <xs:element name="EstimatedAnnualVolume" type="bdt:EstimatedAnnualVolumeType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Szacowany wolumen roczny</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-068</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Szacowana lub deklarowana wartość średniorocznego zapotrzebowania na energię w [kWh]. W przypadku braku wartości zarejestrowanej w Charakterystyce PP, CSIRE udostępni wartość "0".</xs:documentation>
                    <xs:documentation xml:lang="en">Estimated or declared value of average annual energy demand in [kWh]. If there is no value recorded in the MP characteristics, CSIRE will provide the value "0".</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Harmonogram odczytu -->
            <xs:element name="ReadingSchedule" type="bdt:ReadingScheduleType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Harmonogram odczytu</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-069</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Miesiąc startowy odczytu danych z układu pomiarowo-rozliczeniowego, kod CK0123 dotyczy przypadków Ryczałtu lub sytuacji, gdy dla danego PP nie został określony harmonogram odczytu.</xs:documentation>
                    <xs:documentation xml:lang="en">Start month of reading data from the metering and billing system, code CK0123 applies to cases of Lump Sum or situations where no reading schedule has been specified for the MP.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Rodzaj umowy sieciowej -->
            <xs:element name="NetworkAgreementType" type="bdt:NetworkAgreementType_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Rodzaj umowy sieciowej</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-059</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie rodzaju umowy, na podstawie której świadczona jest usługa dystrybucji energii elektrycznej.</xs:documentation>
                    <xs:documentation xml:lang="en">Determination of the type of agreement under which the electricity distribution service is provided.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Status umowy obrotu -->
            <xs:element name="TradeAgreementStatus" type="bdt:TradeAgreementStatusType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Status umowy obrotu</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-090</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie statusu umowy, na podstawie której świadczona jest sprzedaż energii.</xs:documentation>
                    <xs:documentation xml:lang="en">Determination of the status of the agreement under which energy supply is provided.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Status zagrożenia życia, zdrowia i środowiska -->
            <xs:element name="HasThreatToLifeHealthAndEnvironment" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Status zagrożenia życia, zdrowia i środowiska</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-278</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Status informujący, czy dla danego PPE została zarejestrowana konieczność wyłączenia dostaw energii elektrycznej ze względu na zagrożenie życia, zdrowia i środowiska, status ten możliwy do zmiany jedynie dla Operatora.</xs:documentation>
                    <xs:documentation xml:lang="en">Status indicating whether the necessity to turn off electricity supply due to a threat to life, health and the environment has been registered for a given PPE, a status that can only be changed by the Operator.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Status wnioskowanego wstrzymania Operatora -->
            <xs:element name="HasOperatorRequestedSuspension" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Status wnioskowanego wstrzymania Operatora</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-081</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Status informujący, czy dla danego PPE została zarejestrowana konieczność wstrzymania dostaw energii elektrycznej na wniosek Operatora.</xs:documentation>
                    <xs:documentation xml:lang="en">Status indicating whether the need to stop electricity supply at the request of the Operator has been registered for a given MP.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Status wnioskowanego wstrzymania SE -->
            <xs:element name="HasSupplierRequestedSuspension" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Status wnioskowanego wstrzymania SE</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-080</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Status informujący, czy dla danego PPE została zarejestrowana konieczność wstrzymania dostaw energii elektrycznej na wniosek SE.</xs:documentation>
                    <xs:documentation xml:lang="en">Status indicating whether the need to stop electricity supply at the request of the ES has been registered for a given PPE.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Status wnioskowanego wyłączenia Operatora -->
            <xs:element name="HasOperatorRequestedDeactivation" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Status wnioskowanego wyłączenia Operatora</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-276</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Status informujący, czy dla danego PPE została zarejestrowana konieczność wyłączenia dostaw energii elektrycznej na wniosek Operatora.</xs:documentation>
                    <xs:documentation xml:lang="en">Status indicating whether the need to shut down electricity supply at the request of the Operator has been registered for a given PPE.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Status wnioskowanego wyłączenia SE -->
            <xs:element name="HasSupplierRequestedDeactivation" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Status wnioskowanego wyłączenia SE</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-277</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Status informujący, czy dla danego PPE została zarejestrowana konieczność wyłączenia dostaw energii elektrycznej na wniosek SE.</xs:documentation>
                    <xs:documentation xml:lang="en">Status indicating whether the need to turn off electricity supply at the request of the ES has been registered for a given PPE.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Sposób rozliczenia mikroinstalacji -->
            <xs:element name="BillingMethodForMicroInstallation" type="bdt:BillingMethodForMicroInstallationType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Sposób rozliczenia mikroinstalacji</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-221</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie sposobu rozliczenia odbiorcy z mikroinstalacją.</xs:documentation>
                    <xs:documentation xml:lang="en">Determination of the method of billing the customer with a micro-installation.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Zgoda na udostępnienie rzeczywistego profilu zużycia SE -->
            <xs:element name="HasConsumptionProfileConsent" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Zgoda na udostępnienie rzeczywistego profilu zużycia SE</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-067</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Zgoda wyrażona przez odbiorcę na rzecz Sprzedawcy lub wynikająca z regulacji prawnych dotycząca udostępniania rzeczywistego profilu zużycia energii elektrycznej dla PPE. Dotyczy tylko przypadków, gdy Użytkownikiem KSE jest Odbiorca jako osoba fizyczna.</xs:documentation>
                    <xs:documentation xml:lang="en">Consent expressed by the customer to the Energy Supplier or resulting from legal regulations regarding the release of the actual electricity consumption profile for the PPE. Applies only when the KSE User is the Customer as an individual.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Parametry handlowe - warunki świadczenia usług -->
            <xs:element name="BusinessData_TermsOfService" type="unk_3_1_1_1:BusinessData_TermsOfService" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Parametry handlowe - warunki świadczenia usług</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-321</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 8.1.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Parametry handlowe - umowa dystrybucyjna/przesyłowa -->
            <xs:element name="BusinessData_DistributionAgreement" type="unk_3_1_1_1:BusinessData_DistributionAgreement" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Parametry handlowe - umowa dystrybucyjna/przesyłowa</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-317</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 8.2.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Parametry handlowe - umowa sprzedaży -->
            <xs:element name="BusinessData_SupplyAgreement" type="unk_3_1_1_1:BusinessData_SupplyAgreement" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Parametry handlowe - umowa sprzedaży</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-318</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 8.3.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Parametry handlowe - umowa kompleksowa -->
            <xs:element name="BusinessData_ComplexAgreement" type="unk_3_1_1_1:BusinessData_ComplexAgreement" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Parametry handlowe - umowa kompleksowa</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-322</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 8.4.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Parametry handlowe - dodatkowe US/UK -->
            <xs:element name="BusinessData_SupplyAndComplexAgreement" type="unk_3_1_1_1:BusinessData_SupplyAndComplexAgreement" minOccurs="0" maxOccurs="5">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Parametry handlowe - dodatkowe US/UK</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-319</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 8.5.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Parametry handlowe - zarządzanie dostarczaniem energii elektrycznej -->
            <xs:element name="BusinessData_EnergyDeliveryManagement" type="unk_3_1_1_1:BusinessData_EnergyDeliveryManagement" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Parametry handlowe - zarządzanie dostarczaniem energii elektrycznej</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-323</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 8.6.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <!-- ====================================================================================================================================================== -->
    <!-- ==== Parametry handlowe - warunki świadczenia usług                                                                                               ==== -->
    <!-- ====================================================================================================================================================== -->
    <xs:element name="BusinessData_TermsOfService" type="unk_3_1_1_1:BusinessData_TermsOfService"/>
    <xs:complexType name="BusinessData_TermsOfService">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>Parametry handlowe - warunki świadczenia usług</unk_3_1_1_1:plName>
                <unk_3_1_1_1:code>PL-321</unk_3_1_1_1:code>
            </xs:appinfo>
            <xs:documentation xml:lang="pl">Sekcja 8.1.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- Grupa taryfowa -->
            <xs:element name="TariffGroup" type="bdt:String10_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Grupa taryfowa</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-085</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Grupa taryfowa zgodna z Taryfą OSD.</xs:documentation>
                    <xs:documentation xml:lang="en">Tariff group in accordance with the DSO Tariff.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Moc umowna -->
            <xs:element name="ContractedPower" type="bdt:PowerKiloWattsType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Moc umowna</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-086</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Wielkość mocy umownej, wyrażona w [kW].</xs:documentation>
                    <xs:documentation xml:lang="en">The size of the contracted power, expressed in [kW].</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Standardowy profil zużycia -->
            <xs:element name="StandardConsumptionProfile" type="bdt:StandardConsumptionProfileType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Standardowy profil zużycia</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-089</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Standardowy profil zużycia, zgodny z IRiESD OSD.</xs:documentation>
                    <xs:documentation xml:lang="en">Standard consumption profile, in accordance with the DSO's IRiESD.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Czy posiada szczególne warunki świadczenia usług? -->
            <xs:element name="HasSpecificServiceConditions" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Czy posiada szczególne warunki świadczenia usług?</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-118</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie, czy dla danego Punktu poboru energii lub obiektu zachodzą nietypowe warunki świadczenia usług.</xs:documentation>
                    <xs:documentation xml:lang="en">Determination whether abnormal service conditions exist for a given MP or Facility.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Szczególne warunki świadczenia usług -->
            <xs:element name="SpecificTermsOfService" type="bdt:String250_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Szczególne warunki świadczenia usług</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-370</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie szczegółowych warunków świadczenia usług.</xs:documentation>
                    <xs:documentation xml:lang="en">Determination of detailed service conditions.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Jednorazowa przerwa planowana -->
            <xs:element name="PlannedOneOffInterruption" type="bdt:PositiveInteger4_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Jednorazowa przerwa planowana</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-371</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Dopuszczalny czas pojedynczej planowanej przerwy (nie uwzględniająca warunków szczególnych jeżeli zostały określone) w dostarczaniu energii elektrycznej, określony w godzinach.</xs:documentation>
                    <xs:documentation xml:lang="en">Permissible time of a single planned interruption (not taking into account special conditions if specified) in the supply of electricity, specified in hours.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Jednorazowa przerwa nieplanowana -->
            <xs:element name="UnplannedOneOffInterruption" type="bdt:PositiveInteger4_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Jednorazowa przerwa nieplanowana</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-116</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Dopuszczalny czas pojedynczej nieplanowanej przerwy (nie uwzględniająca warunków szczególnych jeżeli zostały określone) w dostarczaniu energii elektrycznej, określony w godzinach.</xs:documentation>
                    <xs:documentation xml:lang="en">Permissible time of a single unplanned interruption (not including special conditions if specified) in the supply of electricity, specified in hours.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Suma przerw planowanych w ciągu roku -->
            <xs:element name="SumOfPlannedInterruptionsDuringTheYear" type="bdt:PositiveInteger4_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Suma przerw planowanych w ciągu roku</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-117</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Suma czasów przerw planowanych (nie uwzględniająca warunków szczególnych jeżeli zostały określone) w ciągu roku, określona w godzinach.</xs:documentation>
                    <xs:documentation xml:lang="en">The sum of the times of scheduled breaks (not taking into account specific conditions if specified) during the year, specified in hours.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Suma przerw nieplanowanych w ciągu roku -->
            <xs:element name="SumOfUnplannedInterruptionsDuringTheYear" type="bdt:PositiveInteger4_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Suma przerw nieplanowanych w ciągu roku</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-087</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Suma czasów przerw nieplanowanych (nie uwzględniająca warunków szczególnych jeżeli zostały określone) w ciągu roku, określona w godzinach.</xs:documentation>
                    <xs:documentation xml:lang="en">The sum of times of unscheduled breaks (not including specific conditions if specified) during the year, specified in hours.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Przekroczenie mocy umownej -->
            <xs:element name="HasExcessPowerBilling" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Przekroczenie mocy umownej</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-350</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określa, czy dla danego PP stosowane jest rozliczanie za przekroczenie mocy umownej.</xs:documentation>
                    <xs:documentation xml:lang="en">Determines whether billing for over contracted power is applied for a given MP.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Ponadnormatywny pobór energii biernej -->
            <xs:element name="HasExcessReactivePowerBilling" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Ponadnormatywny pobór energii biernej</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-351</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określa, czy dla danego PP stosowane jest rozliczanie za ponad umowny pobór energii biernej.</xs:documentation>
                    <xs:documentation xml:lang="en">Determines whether billing for over contracted reactive energy consumption is applied for a given MP.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- tg φ0 Q1 -->
            <xs:element name="TgPhiQ1" type="bdt:TgPhiValue_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>tg φ0 Q1</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-480</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Wartość współczynnika tg φ0 w pierwszym kwadrancie.</xs:documentation>
                    <xs:documentation xml:lang="en">The value of the tg φ0 coefficient in the first quadrant.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- tg φ0 Q2 -->
            <xs:element name="TgPhiQ2" type="bdt:TgPhiValue_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>tg φ0 Q2</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-481</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Wartość współczynnika tg φ0 w drugim kwadrancie.</xs:documentation>
                    <xs:documentation xml:lang="en">The value of the tg φ0 coefficient in the second quadrant.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- tg φ0 Q3 -->
            <xs:element name="TgPhiQ3" type="bdt:TgPhiValue_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>tg φ0 Q3</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-482</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Wartość współczynnika tg φ0 w trzecim kwadrancie.</xs:documentation>
                    <xs:documentation xml:lang="en">Value of tg φ0 coefficient in the third quadrant.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- tg φ0 Q4 -->
            <xs:element name="TgPhiQ4" type="bdt:TgPhiValue_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>tg φ0 Q4</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-483</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Wartość współczynnika tg φ0 w czwartym kwadrancie.</xs:documentation>
                    <xs:documentation xml:lang="en">The value of the tg φ0 coefficient in the fourth quadrant.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <!-- ====================================================================================================================================================== -->
    <!-- ==== Parametry handlowe - umowa dystrybucyjna/przesyłowa                                                                                          ==== -->
    <!-- ====================================================================================================================================================== -->
    <xs:element name="BusinessData_DistributionAgreement" type="unk_3_1_1_1:BusinessData_DistributionAgreement"/>
    <xs:complexType name="BusinessData_DistributionAgreement">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>Parametry handlowe - umowa dystrybucyjna/przesyłowa</unk_3_1_1_1:plName>
                <unk_3_1_1_1:code>PL-317</unk_3_1_1_1:code>
            </xs:appinfo>
            <xs:documentation xml:lang="pl">Sekcja 8.2.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- Data rozpoczęcia -->
            <xs:element name="StartDate" type="xs:date" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Data rozpoczęcia</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-071</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Data przypisania UD albo UP w charakterystyce PP. Od tego okresu możliwe jest przypisanie US do charakterystyki PP.</xs:documentation>
                    <xs:documentation xml:lang="en">Date of assignment of either UD or UP in the MP characteristic. From this period it is possible to assign US to MP characteristic.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Okres wypowiedzenia -->
            <xs:element name="NoticePeriod" type="bdt:String100_Type" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Okres wypowiedzenia</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-072</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Okres wypowiedzenia UD albo UP.</xs:documentation>
                    <xs:documentation xml:lang="en">UD or UP termination period.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Terminowość -->
            <xs:element name="EndDateType" type="bdt:EndDateType_Type" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Terminowość</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-073</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Wskazanie, czy UD albo UP została zawarta na określony czas.</xs:documentation>
                    <xs:documentation xml:lang="en">Indication of whether the UD or UP was concluded for a specific period of time.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Termin zakończenia określony w umowie -->
            <xs:element name="ExpectedEndDate" type="xs:date" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Termin zakończenia określony w umowie</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-074</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Przewidywana data zakończenia UD albo UP, która została określona w umowie (włącznie).</xs:documentation>
                    <xs:documentation xml:lang="en">The expected date of termination of either UD or UP, which is specified in the agreement (inclusive).</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Okres rozliczeniowy -->
            <xs:element name="BillingPeriod" type="bdt:BillingPeriod_Type" minOccurs="1" maxOccurs="1">
