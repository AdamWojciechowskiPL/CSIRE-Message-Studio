    <!-- ====================================================================================================================================================== -->
    <!-- ==== Parametry techniczne - ogólne                                                                                                                ==== -->
    <!-- ====================================================================================================================================================== -->
    <xs:element name="TechnicalData_Basic" type="unk_3_1_1_1:TechnicalData_Basic"/>
    <xs:complexType name="TechnicalData_Basic">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>Parametry techniczne - ogólne</unk_3_1_1_1:plName>
                <unk_3_1_1_1:code>PL-304</unk_3_1_1_1:code>
            </xs:appinfo>
            <xs:documentation xml:lang="pl">Sekcja 4.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- Status przyłączenia PP -->
            <xs:element name="ConnectionStatus" type="bdt:MeteringPointConnectionStatusType" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Status przyłączenia PP</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-095</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie statusu przyłączenia PP, wyróżniamy pięć statusów (UWAGA przy tworzeniu PP inicjalnie mogą być wskazane TYLKO pierwsze 3):
"W trakcie budowy" - gdzie kod PP został nadany, nie wszystkie parametry techniczne i dane PP zostały uzupełnione, nie można realizować umów na takim PP;
"Przyłączony" - warunki określone w umowie przyłączenia zostały zrealizowane oraz obiekt został przyłączony do sieci, PP jest dostępny dla rynku, tylko w tym przypadku jest możliwość realizacji umów sprzedaży i dystrybucji;
"Odłączony" - PP jest tymczasowo niedostępny dla rynku, nie ma możliwości zgłaszania nowych umów dla PP;
"Zlikwidowany" - PP jest trwale niedostępny dla rynku, obiekt został zlikwidowany, zburzony itp;
"Błędny" - stosowany wyłącznie do PP nieprawidłowo utworzonych w CSIRE.</xs:documentation>
                    <xs:documentation xml:lang="en">Determine the status of MP connection, there are five statuses (NOTE when creating a MP initially, ONLY the first 3 can be indicated):
"Under construction" - where the MP code has been assigned, not all technical parameters and MP data have been completed, agreements cannot be executed on such MP;
"Connected" - the conditions specified in the connection agreement have been fulfilled and the facility has been connected to the network, the MP is available to the market, only in this case it is possible to execute supply and distribution agreement;
"Disconnected" - MP is temporarily unavailable to the market, it is not possible to submit new agreements for MP;
"Decommissioned" - MP is permanently unavailable to the market, the facility has been decommissioned, demolished, etc.;
"Incorrect" - applied only to MPs incorrectly created in CSIRE.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Status fizyczny dostaw PP -->
            <xs:element name="PhysicalStatus" type="bdt:PhysicalStatusType" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Status fizyczny dostaw PP</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-082</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Status informujący, czy dane PP jest załączone (jest napięcie) lub wyłączone (brak napięcia).</xs:documentation>
                    <xs:documentation xml:lang="en">Status indicating whether a given MP is on (there is voltage) or off (no voltage).</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Możliwość wstrzymania windykacyjnego -->
            <xs:element name="CanBeSuspendedForDebtCollection" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Możliwość wstrzymania windykacyjnego</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-055</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie, czy dla danego Punktu poboru energii dopuszczalne jest wstrzymywanie dostaw energii.</xs:documentation>
                    <xs:documentation xml:lang="en">Determination whether withholding of energy supply is allowed for a given Energy Point.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Poziom napięcia w miejscu dostarczania -->
            <xs:element name="MeteringPointElectricityVoltageLevel" type="bdt:MeasurementCircuitVoltageLevelType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Poziom napięcia w miejscu dostarczania</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-092</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Poziom napięcia w miejscu dostarczania.</xs:documentation>
                    <xs:documentation xml:lang="en">Voltage level at the point of delivery.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Fazowość układu pomiarowego -->
            <xs:element name="MeasuringSystemPhasesCount" type="bdt:MeasuringSystemPhasesCountType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Fazowość układu pomiarowego</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-093</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Fazowość układu pomiarowo-rozliczeniowego.</xs:documentation>
                    <xs:documentation xml:lang="en">Phasing of the metering and billing system.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Minimalna moc umowna -->
            <xs:element name="MinContractedPower" type="bdt:PowerKiloWattsType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Minimalna moc umowna</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-110</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Wartość minimalnej mocy umownej, wyrażona w [kW].</xs:documentation>
                    <xs:documentation xml:lang="en">The value of the minimum contracted capacity, expressed in [kW].</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Maksymalna moc umowna -->
            <xs:element name="MaxContractedPower" type="bdt:PowerKiloWattsType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Maksymalna moc umowna</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-111</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Wartość maksymalnej mocy umownej, wyrażona w [kW].</xs:documentation>
                    <xs:documentation xml:lang="en">The value of the maximum contracted power, expressed in [kW].</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Grupa przyłączeniowa -->
            <xs:element name="ConnectionGroup" type="bdt:ConnectionGroupType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Grupa przyłączeniowa</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-101</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Grupa przyłączeniowa</xs:documentation>
                    <xs:documentation xml:lang="en">Connection group</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Data obowiązywania warunków przyłączeniowych -->
            <xs:element name="DateOfValidityOfTheConnectionConditions" type="xs:date" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Data obowiązywania warunków przyłączeniowych</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-098</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określana jedynie dla VI gr. przyłączeniowej, wskazuje ostatni dzień warunków przyłączenia.</xs:documentation>
                    <xs:documentation xml:lang="en">Specified only for the VI connection gr. and indicates the last day of the connection conditions.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Moc przyłączeniowa -->
            <xs:element name="ConnectionPower" type="bdt:PowerKiloWattsType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Moc przyłączeniowa</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-102</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Wartość mocy przyłączeniowej, wyrażona w [kW].</xs:documentation>
                    <xs:documentation xml:lang="en">The value of the connection power, expressed in [kW].</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Miejsce dostarczania energii -->
            <xs:element name="PlaceOfEnergySupply" type="bdt:String250_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Miejsce dostarczania energii</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-103</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Miejsce dostarczania energii</xs:documentation>
                    <xs:documentation xml:lang="en">Place of energy supply</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Miejsce rozgraniczenia własności urządzeń -->
            <xs:element name="DeviceOwnershipSeparationLocation" type="bdt:String250_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Miejsce rozgraniczenia własności urządzeń</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-104</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Miejsce rozgraniczenia własności urządzeń.</xs:documentation>
                    <xs:documentation xml:lang="en">Place of separation of ownership of equipment.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Miejsce zainstalowania układu pomiarowo-rozliczeniowego -->
            <xs:element name="PlaceOfInstallationOfMeteringAndBillingSystem" type="bdt:String250_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Miejsce zainstalowania układu pomiarowo-rozliczeniowego</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-515</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Miejsce zainstalowania układu pomiarowo-rozliczeniowego.</xs:documentation>
                    <xs:documentation xml:lang="en">Place of installation of the metering and billing system.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Współczynnik pewności zasilania -->
            <xs:element name="PowerReliabilityFactor" type="bdt:PowerReliabilityFactorType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Współczynnik pewności zasilania</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-112</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Współczynnik odzwierciedlający specyfikę układu zasilania Użytkownika KSE. Domyślna wartość współczynnika równa się 1.</xs:documentation>
                    <xs:documentation xml:lang="en">A factor reflecting the specifics of the KSE User's power system. The default value of the coefficient is equal to 1.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Wartość zabezpieczenia przed licznikowego -->
            <xs:element name="FuseSize" type="xs:integer" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Wartość zabezpieczenia przed licznikowego</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-094</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Wartość zabezpieczenia przed licznikowego, wyrażona w [A]. Atrybut obsługujący również przypadek zabezpieczenia dla grupy taryfowej R - Ryczałt.
W przypadku układów pomiarowo-rozliczeniowych półpośrednich i pośrednich należy wprowadzić zero "0".</xs:documentation>
                    <xs:documentation xml:lang="en">The value of pre-meter protection, expressed in [A]. The attribute also supports the case of R tariff group - Lump Sum.
In the case of semi-direct and indirect metering and billing systems, zero value should be used "0".</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Czy posiada dodatkowe nośniki energii? -->
            <xs:element name="HasAdditionalEnergyCarriers" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Czy posiada dodatkowe nośniki energii?</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-543</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie, czy dany Punkt poboru energii posiada dodatkowe nośniki energii, niewynikające z Charakteru PPE.</xs:documentation>
                    <xs:documentation xml:lang="en">Determination whether the Energy Point has additional energy carriers, not arising from the Nature of the PPE.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Parametry techniczne - licznik -->
            <xs:element name="TechnicalData_Meter" type="unk_3_1_1_1:TechnicalData_Meter" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Parametry techniczne - licznik</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-305</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 4.1.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Parametry techniczne - analizator JEE -->
            <xs:element name="TechnicalData_EeqValidator" type="unk_3_1_1_1:TechnicalData_EeqValidator" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Parametry techniczne - analizator JEE</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-517</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 4.2.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Parametry techniczne - przekładniki -->
            <xs:element name="TechnicalData_Transformers" type="unk_3_1_1_1:TechnicalData_Transformers" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Parametry techniczne - przekładniki</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-306</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 4.3.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Parametry techniczne - straty -->
            <xs:element name="TechnicalData_Losses" type="unk_3_1_1_1:TechnicalData_Losses" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Parametry techniczne - straty</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-307</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 4.4.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Parametry techniczne - nośniki energii -->
            <xs:element name="TechnicalData_EnergyCarriers" type="unk_3_1_1_1:TechnicalData_EnergyCarriers" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Parametry techniczne - nośniki energii</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-544</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 4.5.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <!-- ====================================================================================================================================================== -->
    <!-- ==== Parametry techniczne - licznik                                                                                                               ==== -->
    <!-- ====================================================================================================================================================== -->
    <xs:element name="TechnicalData_Meter" type="unk_3_1_1_1:TechnicalData_Meter"/>
    <xs:complexType name="TechnicalData_Meter">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>Parametry techniczne - licznik</unk_3_1_1_1:plName>
                <unk_3_1_1_1:code>PL-305</unk_3_1_1_1:code>
            </xs:appinfo>
            <xs:documentation xml:lang="pl">Sekcja 4.1.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- Typ układu pomiarowo-rozliczeniowego -->
            <xs:element name="MeasurementAndBillingCircuitType" type="bdt:MeasurementAndBillingCircuitType_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Typ układu pomiarowo-rozliczeniowego</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-096</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie jakie wymagania, zawarte w rozporządzeniu w sprawie systemu pomiarowego, spełnia układ pomiarowo-rozliczeniowy zainstalowany w danym PP.
Kod CK0201 dotyczy układów spełniających wszystkie wymogi rozporządzenia ws. systemu pomiarowego.
Kody CK0202 i CK0203 oznaczają układy częściowo spełniające wymogi tego rozporządzenia (np. posiadające zdalną transmisję danych, ale nieposiadające stycznika).
Wartości CK0201 dla kategorii C1 lub CK0202, gdy komunikacja LZO=True i metoda pomiaru=E13 tego atrybutu, oznaczają możliwość uruchomienia rozliczenia PPE w trybie przedpłatowym.</xs:documentation>
                    <xs:documentation xml:lang="en">Determining what requirements contained in the regulation on the measurement system are met by the measurement and billing system installed in a given MP.
The CK0201 code applies to systems that meet all the requirements of the measurement system regulation.
The CK0202 and CK0203 codes refer to systems that partially meet the requirements of this regulation (e.g. with remote data transmission, but without a contactor).
The values CK0201 for C1 category and CK0202 (only when IsSmartMeterConnected=True and measurement method=E13) of this attribute indicate the possibility of starting PPE billing in the prepayment mode.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Komunikacja LZO -->
            <xs:element name="IsSmartMeterConnected" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Komunikacja LZO</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-058</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Atrybut określa, czy Operator skomunikował licznik zdalnego odczytu z systemem pomiarowym, w sposób umożliwiający wymianę informacji wynikającą z rozporządzenia w sprawie systemu pomiarowego. Atrybut ten powinien być określony statycznie, nie referować do tymczasowej utraty komunikacji z LZO. Atrybut ten jest informacją dla SE o możliwości zarządzania dostarczaniem en.el. w PPE oraz możliwością uruchomienia rozliczenia PPE w trybie przedpłatowym.</xs:documentation>
                    <xs:documentation xml:lang="en">The attribute specifies whether the Operator has communicated the remote reading meter with the metering system, in a way that allows the exchange of information resulting from the Metering System Regulation. This attribute should be determined statically, not refer to a temporary loss of communication with the Smart Meter. This attribute is information to the ES about the ability to manage the supply of electrical energy in the PPE and the ability to trigger PPE prepayment billing.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Nr licznika -->
            <xs:element name="MeterNumber" type="bdt:MeterNumberType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Nr licznika</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-097</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Numer seryjny podstawowego licznika energii czynnej.</xs:documentation>
                    <xs:documentation xml:lang="en">Serial number of the primary active energy meter.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Metoda pomiaru -->
            <xs:element name="MeasurementMethod" type="bdt:MeasurementMethod_Type" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Metoda pomiaru</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-099</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie metody pomiaru, zgodnie z najbardziej zaawansowaną i wykorzystywaną w danym PP funkcjonalnością licznika energii elektrycznej. Dla PL-096=CK0201 należy wskazać E13, dla PL-096=CK0202 i CK0203 wartość E13 należy wskazać jeżeli układ pomiarowo-rozliczeniowy umożliwia transmisję  danych o profilu zużycia (a nie tylko jego rejestrowanie). W przypadku CK0205 należy wskazać E16.</xs:documentation>
                    <xs:documentation xml:lang="en">Determining the measurement method in accordance with the most advanced functionality of the electricity meter used in a given MP. For PL-096=CK0201, E13 should be used, for PL-096=CK0202 and CK0203, the E13 value should be used if the measurement and billing system enables the transmission of data on the consumption profile (and not only its recording). In the case of CK0205, use E16.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Status dostosowania układu pomiarowego -->
            <xs:element name="MeteringSystemAdjustmentStatus" type="bdt:MeasurementCircuitAdaptationStatusType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Status dostosowania układu pomiarowego</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-105</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określa, czy układ pomiarowo-rozliczeniowy jest dostosowany (zgodnie z IRiESD OSD) do TPA. Przy czym kod: 
CK0341 - stosuje się dla układów pomiarowo-rozliczeniowych, które są dostosowane lub nie muszą być dostosowane. Kod ten nie blokuje zmiany sprzedawcy;
CK0342 - stosuje się gdy układ pomiarowo-rozliczeniowy jest niedostosowany, w tym przypadku nie ma możliwości zmiany sprzedawcy;
CK0050 - stosowana dla ryczałtu oraz PPE w sieci przesyłowej.</xs:documentation>
                    <xs:documentation xml:lang="en">Determines whether the metering and billing system is adjusted (according to the DSO's IRiESD) to TPA. Whereby the code:
CK0341 - is used for metering and billing systems that are adjusted or do not need to be adjusted. This code does not block the change of Energy Supplier; 
CK0342 - used when the metering and billing system is unadjusted, in which case there is no possibility to change the Energy Supplier;
CK0050 - used for lump sum and PPE in the transmission network.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Poziom napięcia zainstalowania układu pomiarowego -->
            <xs:element name="MeteringSystemInstallationVoltageLevel" type="bdt:MeasurementCircuitVoltageLevelType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Poziom napięcia zainstalowania układu pomiarowego</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-091</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Poziom napięcia zainstalowania układu pomiarowo-rozliczeniowego.</xs:documentation>
                    <xs:documentation xml:lang="en">The voltage level of the installation of the metering and billing system.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Parametry licznika - rejestry pomiarowe -->
            <xs:element name="MeterData_Registers" type="unk_3_1_1_1:MeterData_Registers" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Parametry licznika - rejestry pomiarowe</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-119</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 4.1.1.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Parametry licznika - typ produktu energetycznego -->
            <xs:element name="MeterData_ProductType" type="unk_3_1_1_1:MeterData_ProductType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Parametry licznika - typ produktu energetycznego</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-248</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 4.1.2.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <!-- ====================================================================================================================================================== -->
    <!-- ==== Parametry licznika - rejestry pomiarowe                                                                                                      ==== -->
    <!-- ====================================================================================================================================================== -->
    <xs:element name="MeterData_Registers" type="unk_3_1_1_1:MeterData_Registers"/>
    <xs:complexType name="MeterData_Registers">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>Parametry licznika - rejestry pomiarowe</unk_3_1_1_1:plName>
                <unk_3_1_1_1:code>PL-119</unk_3_1_1_1:code>
            </xs:appinfo>
            <xs:documentation xml:lang="pl">Sekcja 4.1.1.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- Typ rejestru -->
            <xs:element name="RegisterType" type="bdt:MeterRegistryType_Type" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Typ rejestru</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-088</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Zgodnie z opisem protokołu IEC62056, atrybut wskazuje rejestr pomiarowy licznika energii elektrycznej, którego dotyczy przekazywanie wskazań pomiarowych.</xs:documentation>
                    <xs:documentation xml:lang="en">According to the description of the IEC62056 protocol, the attribute indicates the measurement register of the electricity meter to which the transmission of measurement readings applies.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <!-- ====================================================================================================================================================== -->
    <!-- ==== Parametry licznika - typ produktu energetycznego                                                                                             ==== -->
    <!-- ====================================================================================================================================================== -->
    <xs:element name="MeterData_ProductType" type="unk_3_1_1_1:MeterData_ProductType"/>
    <xs:complexType name="MeterData_ProductType">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>Parametry licznika - typ produktu energetycznego</unk_3_1_1_1:plName>
                <unk_3_1_1_1:code>PL-248</unk_3_1_1_1:code>
            </xs:appinfo>
            <xs:documentation xml:lang="pl">Sekcja 4.1.2.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- Typ produktu energetycznego -->
            <xs:element name="ProductType" type="bdt:ProductType_Type" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Typ produktu energetycznego</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-247</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie właściwego produktu energetycznego, zgodnie z listą. 
W zakresie wskazania profilów pomiarowych, oznacza przekazanie danych profilowych bez uwzględnienia doliczeń/odliczeń, w odróżnieniu do wskazania profilów rozliczeniowego, który uwzględnia w prezentowanych wartościach straty energii elektrycznej lub doliczenia oraz odliczenia, zgodnie z parametrami umowy między Operatorem a Użytkownikiem KSE. 
Produkt energetyczny: Energia pobrana (CK1125, CK1128), dotyczy wielkości pobranej z sieci elektroenergetycznej; Energia wprowadzona (CK1126, CK1129) dotyczy wielkości wprowadzonej do sieci.
Produkt energetyczny: Bilans energii czynnej (CK1127, CK1130) dotyczy wielkości wyliczonej jako sumaryczne zbilansowanie w godzinie (t) ilości energii wprowadzonej do i pobranej z sieci z wszystkich faz. Wartości dodatnie (+) oznaczają energię pobraną z sieci, wartości ujemne bilansu energii czynnej (-) oznacza energię wprowadzoną do sieci.</xs:documentation>
                    <xs:documentation xml:lang="en">Determination of the relevant energy product, according to the list. 
In terms of the metering profiles measurements, means the transmission of profile data without taking into account additions/deductions, as opposed to the billing profiles measurements, which takes into account electricity losses or additions and deductions in the presented values, in accordance with the parameters of the agreement between the Operator and the KSE User.
Energy product: Energy withdrawn (CK1125, CK1128), refers to the volume withdrawn from the power grid; Energy injected (CK1126, CK1129) refers to the volume injected into the grid.
Energy product: Active energy balance (CK1127, CK1130) refers to the quantity calculated as the summed balance in hour (t) of the amount of energy injected into and withdrawn from the grid from all phases. Positive values (+) represent energy taken from the grid, negative values of the active energy balance (-) represent energy fed into the grid.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <!-- ====================================================================================================================================================== -->
    <!-- ==== Parametry techniczne - analizator JEE                                                                                                        ==== -->
    <!-- ====================================================================================================================================================== -->
    <xs:element name="TechnicalData_EeqValidator" type="unk_3_1_1_1:TechnicalData_EeqValidator"/>
    <xs:complexType name="TechnicalData_EeqValidator">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>Parametry techniczne - analizator JEE</unk_3_1_1_1:plName>
                <unk_3_1_1_1:code>PL-517</unk_3_1_1_1:code>
            </xs:appinfo>
            <xs:documentation xml:lang="pl">Sekcja 4.2.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- Rodzaj analizatora JEE -->
            <xs:element name="EeqValidatorType" type="bdt:EeqValidatorType_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Rodzaj analizatora JEE</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-518</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie, czy w PP znajduje się dodatkowo analizator, mierzący parametry jakości energii elektrycznej. W przypadku braku analizatora należy zastosować kod CK0322 - Brak analizatora.</xs:documentation>
                    <xs:documentation xml:lang="en">Determine whether the MP additionally has an analyzer, measuring power quality parameters. If there is no analyzer, use code CK0322 - No analyzer.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Nr analizatora JEE -->
            <xs:element name="EeqValidatorIdentifier" type="bdt:EeqValidatorIdentifierType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Nr analizatora JEE</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-519</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Numer seryjny analizatora JEE.</xs:documentation>
                    <xs:documentation xml:lang="en">Serial number of the EEQ analyzer.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <!-- ====================================================================================================================================================== -->
    <!-- ==== Parametry techniczne - przekładniki                                                                                                          ==== -->
    <!-- ====================================================================================================================================================== -->
    <xs:element name="TechnicalData_Transformers" type="unk_3_1_1_1:TechnicalData_Transformers"/>
    <xs:complexType name="TechnicalData_Transformers">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>Parametry techniczne - przekładniki</unk_3_1_1_1:plName>
                <unk_3_1_1_1:code>PL-306</unk_3_1_1_1:code>
            </xs:appinfo>
            <xs:documentation xml:lang="pl">Sekcja 4.3.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- Przekładnia przekładników prądowych -->
            <xs:element name="CurrentTransformerRatio" type="bdt:String30_Type" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Przekładnia przekładników prądowych</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-106</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Wielkość przekładni przekładników prądowych opisanych stosunkiem przekładni pierwotnej do wtórnej (np.: 1/1, 75/5, 200000/1), w przypadku braku przekładników należy podać wartość "1".</xs:documentation>
                    <xs:documentation xml:lang="en">The magnitude of current transformers described by the ratio of primary to return gear (e.g.: 1/1, 75/5, 200000/1), in the absence of reloaders, the value "1" should be given.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Przekładnia przekładników napięciowych -->
            <xs:element name="VoltageTransformerRatio" type="bdt:String30_Type" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Przekładnia przekładników napięciowych</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-108</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Wielkość przekładni przekładników napięciowych, opisanych stosunkiem przekładni pierwotnej do wtórnej (np.:  6000/100, 30/0,1, (110/√3)/(0,1/√3)  w przypadku braku przekładników należy podać wartość "1"</xs:documentation>
                    <xs:documentation xml:lang="en">The size of voltage transformers, described by the ratio of primary to secondary transformers (e.g.: 6000/100, 30/0.1, (110/√3)/(0.1/√3) if there are no transformers, the value of "1" should be given.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Mnożna przekładników -->
            <xs:element name="TransformerMultiplicationFactor" type="bdt:MultiplierType" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Mnożna przekładników</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-109</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Mnożna wynikająca z przekładni przekładników prądowych i napięciowych, w przypadku braku przekładników należy przyjąć wartość „1”.</xs:documentation>
                    <xs:documentation xml:lang="en">The multiplication factor derived from the current and voltage transformers, if there are no transformers, the value "1" should be taken.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <!-- ====================================================================================================================================================== -->
    <!-- ==== Parametry techniczne - straty                                                                                                                ==== -->
    <!-- ====================================================================================================================================================== -->
    <xs:element name="TechnicalData_Losses" type="unk_3_1_1_1:TechnicalData_Losses"/>
    <xs:complexType name="TechnicalData_Losses">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>Parametry techniczne - straty</unk_3_1_1_1:plName>
                <unk_3_1_1_1:code>PL-307</unk_3_1_1_1:code>
            </xs:appinfo>
            <xs:documentation xml:lang="pl">Sekcja 4.4.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- Rodzaj strat -->
            <xs:element name="EnergyLossType" type="bdt:EnergyLossType_Type" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Rodzaj strat</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-113</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określa rodzaj strat uwzględnianych w rozliczeniu en.el. w miejscu dostarczania, w przypadku braku strat należy skorzystać z kodu CK1119.</xs:documentation>
                    <xs:documentation xml:lang="en">Determines the type of losses taken into account in the billing of electrical energy at the place of delivery, in case of no losses, use code CK1119.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Kategoria strat -->
            <xs:element name="EnergyLossCategory" type="bdt:EnergyLossCategoryType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Kategoria strat</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-114</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Informacja nt. kategorii strat, uzupełniana zgodnie z wartościami słownikowymi (doliczanie i odliczanie dla kierunku pobór).</xs:documentation>
                    <xs:documentation xml:lang="en">Information on the category of losses, completed according to the dictionary values (addition and deduction for the direction of collection).</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Parametry techniczne - straty procentowe -->
            <xs:element name="TechnicalData_PercentLosses" type="unk_3_1_1_1:TechnicalData_PercentLosses" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Parametry techniczne - straty procentowe</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-536</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 4.4.1.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Parametry techniczne - straty licznikowe -->
            <xs:element name="TechnicalData_MeterLosses" type="unk_3_1_1_1:TechnicalData_MeterLosses" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Parametry techniczne - straty licznikowe</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-537</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 4.4.2.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Parametry techniczne - straty biegu jałowego -->
            <xs:element name="TechnicalData_IdleModeLosses" type="unk_3_1_1_1:TechnicalData_IdleModeLosses" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Parametry techniczne - straty biegu jałowego</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-356</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Sekcja 4.4.3.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <!-- ====================================================================================================================================================== -->
    <!-- ==== Parametry techniczne - straty procentowe                                                                                                     ==== -->
    <!-- ====================================================================================================================================================== -->
    <xs:element name="TechnicalData_PercentLosses" type="unk_3_1_1_1:TechnicalData_PercentLosses"/>
    <xs:complexType name="TechnicalData_PercentLosses">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>Parametry techniczne - straty procentowe</unk_3_1_1_1:plName>
                <unk_3_1_1_1:code>PL-536</unk_3_1_1_1:code>
            </xs:appinfo>
            <xs:documentation xml:lang="pl">Sekcja 4.4.1.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- Wartość strat % mocy -->
            <xs:element name="EnergyLossValue" type="bdt:PercentageType" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Wartość strat % mocy</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-115</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Wartość strat procentowych mocy, można podać wartości dodatnie i ujemne.</xs:documentation>
                    <xs:documentation xml:lang="en">The value of power percentage loss, positive and negative values can be given.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Wartość strat % energii czynnej -->
            <xs:element name="EnergyLossValueForActivePower" type="bdt:PercentageType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Wartość strat % energii czynnej</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-120</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Wartość strat procentowych energii czynnej, można podać wartości dodatnie i ujemne.</xs:documentation>
                    <xs:documentation xml:lang="en">The value of percentage losses of active energy, positive and negative values can be given.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Wartość strat % energii biernej -->
            <xs:element name="EnergyLossValueForReactivePower" type="bdt:PercentageType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Wartość strat % energii biernej</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-121</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Wartość strat procentowych energii biernej, można podać wartości dodatnie i ujemne.</xs:documentation>
                    <xs:documentation xml:lang="en">Value of percentage loss of reactive energy, positive and negative values can be specified.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Wartość strat % energii biernej od energii czynnej -->
            <xs:element name="EnergyLossValueForReactivePowerAgainstActivePower" type="bdt:PercentageType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Wartość strat % energii biernej od energii czynnej</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-122</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Wartość strat procentowych energii czynnej do naliczenia wolumenu energii biernej, można podać wartości ujemne i dodatnie.</xs:documentation>
                    <xs:documentation xml:lang="en">The value of percentage loss of active energy for reactive energy volume calculation, negative and positive values can be specified.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <!-- ====================================================================================================================================================== -->
    <!-- ==== Parametry techniczne - straty licznikowe                                                                                                     ==== -->
    <!-- ====================================================================================================================================================== -->
    <xs:element name="TechnicalData_MeterLosses" type="unk_3_1_1_1:TechnicalData_MeterLosses"/>
    <xs:complexType name="TechnicalData_MeterLosses">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>Parametry techniczne - straty licznikowe</unk_3_1_1_1:plName>
                <unk_3_1_1_1:code>PL-537</unk_3_1_1_1:code>
            </xs:appinfo>
            <xs:documentation xml:lang="pl">Sekcja 4.4.2.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- Współczynnik I2h strat licznikowych dla energii czynnej -->
            <xs:element name="I2hMeteringLossFactorForActiveEnergy" type="bdt:LossCoefficientType" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Współczynnik I2h strat licznikowych dla energii czynnej</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-123</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Wartość współczynnika strat licznikowych (prądowych) określonych dla energii czynnej.</xs:documentation>
                    <xs:documentation xml:lang="en">The value of the meter (current) loss coefficient determined for active energy.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Współczynnik U2h strat licznikowych dla energii czynnej -->
            <xs:element name="U2hMeteringLossFactorForActiveEnergy" type="bdt:LossCoefficientType" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Współczynnik U2h strat licznikowych dla energii czynnej</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-124</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Wartość współczynnika strat licznikowych (napięciowych) określonych dla energii czynnej.</xs:documentation>
                    <xs:documentation xml:lang="en">The value of the coefficient of meter (voltage) losses specified for active energy.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Współczynnik I2h strat licznikowych dla energii biernej -->
            <xs:element name="I2hMeteringLossFactorForReactiveEnergy" type="bdt:LossCoefficientType" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Współczynnik I2h strat licznikowych dla energii biernej</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-355</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Wartość współczynnika strat licznikowych (prądowych) określonych dla energii biernej.</xs:documentation>
                    <xs:documentation xml:lang="en">The value of the meter (current) loss coefficient determined for reactive energy.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Współczynnik U2h strat licznikowych dla energii biernej -->
            <xs:element name="U2hMeteringLossFactorForReactiveEnergy" type="bdt:LossCoefficientType" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Współczynnik U2h strat licznikowych dla energii biernej</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-538</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Wartość współczynnika strat licznikowych (napięciowych) określonych dla energii biernej.</xs:documentation>
                    <xs:documentation xml:lang="en">The value of the coefficient of meter (voltage) losses specified for reactive energy.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <!-- ====================================================================================================================================================== -->
    <!-- ==== Parametry techniczne - straty biegu jałowego                                                                                                 ==== -->
    <!-- ====================================================================================================================================================== -->
    <xs:element name="TechnicalData_IdleModeLosses" type="unk_3_1_1_1:TechnicalData_IdleModeLosses"/>
    <xs:complexType name="TechnicalData_IdleModeLosses">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>Parametry techniczne - straty biegu jałowego</unk_3_1_1_1:plName>
                <unk_3_1_1_1:code>PL-356</unk_3_1_1_1:code>
            </xs:appinfo>
            <xs:documentation xml:lang="pl">Sekcja 4.4.3.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- Wartość strat biegu jałowego -->
            <xs:element name="IdleLossValue" type="bdt:IdleModeLossesValueType" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Wartość strat biegu jałowego</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-357</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Wielkość strat biegu jałowego z tytułu niedostosowania mocy znamionowej transformatora do zamówionej mocy umownej [kWh/miesiąc], można podać wartości dodatnie i ujemne.</xs:documentation>
                    <xs:documentation xml:lang="en">The magnitude of idling losses due to the mismatch between the transformer's rated power and the ordered contracted power [kWh/month], positive and negative values can be given.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <!-- ====================================================================================================================================================== -->
    <!-- ==== Parametry techniczne - nośniki energii                                                                                                       ==== -->
    <!-- ====================================================================================================================================================== -->
    <xs:element name="TechnicalData_EnergyCarriers" type="unk_3_1_1_1:TechnicalData_EnergyCarriers"/>
    <xs:complexType name="TechnicalData_EnergyCarriers">
        <xs:annotation>
            <xs:appinfo>
                <unk_3_1_1_1:plName>Parametry techniczne - nośniki energii</unk_3_1_1_1:plName>
                <unk_3_1_1_1:code>PL-544</unk_3_1_1_1:code>
            </xs:appinfo>
            <xs:documentation xml:lang="pl">Sekcja 4.5.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- Typ i rodzaj nośnika -->
            <xs:element name="EnergyCarrierType" type="bdt:EnergyCarrierType_Type" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Typ i rodzaj nośnika</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-545</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie, jaki typ i rodzaj nośnika jest zainstalowany w punkcie pomiarowym.</xs:documentation>
                    <xs:documentation xml:lang="en">Determination of what type and type of carrier is installed at the metering point.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Moc zainstalowana nośnika -->
            <xs:element name="CarrierInstalledPower" type="bdt:RealVolumeType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Moc zainstalowana nośnika</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-494</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Wartość mocy elektrycznej zainstalowanej w nośniku.</xs:documentation>
                    <xs:documentation xml:lang="en">The value of the electric power installed in the carrier.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Jednostka mocy zainstalowanej nośnika -->
            <xs:element name="CarrierPowerUnit" type="bdt:UnitCodeType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Jednostka mocy zainstalowanej nośnika</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-557</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Jednostka mocy zainstalowanej dla nośnika energii. Wyrażona zgodnie z wybranym typem rejestru w [kW], [MW], [kVA], [MVA].</xs:documentation>
                    <xs:documentation xml:lang="en">Unit of installed capacity for the energy carrier. Expressed according to the selected type of register in [kW], [MW], [kVA], [MVA].</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Czy kogeneracja? -->
            <xs:element name="HasCogeneration" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Czy kogeneracja?</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-493</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Określenie, czy energia elektryczna wytwarzana jest razem z ciepłem użytkowym.</xs:documentation>
                    <xs:documentation xml:lang="en">Determine whether electricity is generated together with utility heat.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Data pierwszego wprowadzenia energii z mikroinstalacji -->
            <xs:element name="DateOfFirstIntroductionOfEnergyFromMicroInstallations" type="xs:date" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Data pierwszego wprowadzenia energii z mikroinstalacji</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-127</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Data pierwszego wprowadzenia energii z mikroinstalacji do sieci.</xs:documentation>
                    <xs:documentation xml:lang="en">Date of the first introduction of energy from the microinstallation to the grid.</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Pojemność nominalna magazynu energii elektrycznej -->
            <xs:element name="StorageNominalCapacity" type="bdt:StorageNominalCapacityType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Pojemność nominalna magazynu energii elektrycznej</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-129</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Pojemność nominalna magazynu energii elektrycznej, wyrażona w [kWh].</xs:documentation>
                    <xs:documentation xml:lang="en">Nominal capacity of electricity storage, expressed in [kWh].</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <!-- Sprawność magazynu energii elektrycznej -->
            <xs:element name="StorageNominalEfficiency" type="bdt:PositivePercentageType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <unk_3_1_1_1:plName>Sprawność magazynu energii elektrycznej</unk_3_1_1_1:plName>
                        <unk_3_1_1_1:code>PL-130</unk_3_1_1_1:code>
                    </xs:appinfo>
                    <xs:documentation xml:lang="pl">Znamionowa sprawność cyklu jednokrotnego ładowania, wyrażona w [%].</xs:documentation>
